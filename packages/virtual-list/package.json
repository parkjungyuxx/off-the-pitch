{
  "name": "@bongsik/virtual-list",
  "version": "0.1.2",
  "description": "React hook for virtualized list rendering",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "react",
    "virtual-list",
    "virtualization",
    "virtual-scroll",
    "performance",
    "typescript"
  ],
  "author": "parkjungyuxx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/parkjungyuxx/off-the-pitch.git",
    "directory": "packages/virtual-list"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^8.57.0",
    "jsdom": "^27.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript-eslint": "^7.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  }
}
